{
	"name": "IncrementalLoad",
	"properties": {
		"activities": [
			{
				"name": "GetLastLoad",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "SELECT MAX(LastLoad) AS LastLoad\nFROM ETL.LoadMeta",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "dat_db_DynamicTable",
						"type": "DatasetReference",
						"parameters": {
							"schema": "ETL",
							"table": "LoadMeta"
						}
					}
				}
			},
			{
				"name": "SetLastLoad",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "GetLastLoad",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "LastLoad",
					"value": {
						"value": "@activity('GetLastLoad').output.firstRow.LastLoad",
						"type": "Expression"
					}
				}
			}
		],
		"variables": {
			"LastLoad": {
				"type": "String"
			}
		},
		"folder": {
			"name": "JunioClementiEsercitazione"
		},
		"annotations": []
