				},
				"typeProperties": {
					"scripts": [
						{
							"type": "Query",
							"text": {
								"value": "@concat('update ETL.LoadMeta set LastLoad = ''',\nvariables('WriteDate'),\n''' where Owner = ''AndreaSorbello'' and TableName =''SalesOrderDetail_SorbelloAndrea''')\n\n",
