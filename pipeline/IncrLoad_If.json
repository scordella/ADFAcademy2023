		},
		"variables": {
			"LastLoad": {
				"type": "String"
			},
			"WriteDate": {
				"type": "String"
			},
			"MaxLTDate": {
				"type": "String"
			}
		},
		"folder": {
			"name": "Esercizi"
		},
		"annotations": []
	}
